{
  "name": "upload",
  "version": "0.1.0",
  "description": "A software for data transfers via the cloud",
  "main": "src/index.ts",
  "type": "commonjs",
  "bin": {
    "upload": "./upload.cjs"
  },
  "scripts": {
    "build": "node build.mjs",
    "pkg": "npm run build && pkg --options='max-old-space-size=8192' --public --targets='latest-linuxstatic-x64' --debug upload.cjs",
    "start": "npm run build && node upload.cjs",
    "serve": "concurrently --kill-others './build.mjs watch' 'nodemon --quiet ./upload.cjs serve --port 13481 --public-key-file public.pem'"
  },
  "author": "Lea Waller <lea@lea.science>",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.440.0",
    "@aws-sdk/s3-request-presigner": "^3.440.0",
    "@socket.io/admin-ui": "^0.5.1",
    "@socket.io/cluster-adapter": "^0.2.2",
    "@socket.io/sticky": "^1.0.4",
    "bytes": "^3.1.2",
    "commander": "^11.1.0",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "fastq": "^1.15.0",
    "format-duration": "^3.0.2",
    "gauge": "^5.0.1",
    "got": "^13.0.0",
    "https-proxy-agent": "^7.0.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "memoizee": "^0.4.15",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/bytes": "^3.1.3",
    "@types/debug": "^4.1.10",
    "@types/format-duration": "^1.0.2",
    "@types/gauge": "^5.0.3",
    "@types/got": "^9.6.12",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/memoizee": "^0.4.10",
    "@types/node": "^20.8.10",
    "concurrently": "^8.2.2",
    "esbuild": "^0.20.1",
    "esbuild-plugin-tsc": "^0.4.0",
    "nodemon": "^3.0.1",
    "pkg": "^5.8.1",
    "typescript": "^5.2.2"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8",
    "utf-8-validate": "^5.0.10"
  },
  "pkg": {
    "options": [
      "max-old-space-size=8192"
    ]
  }
}
