{
  "name": "upload",
  "version": "0.1.0",
  "description": "A software for data transfers via the cloud",
  "main": "src/index.ts",
  "type": "module",
  "bin": {
    "upload": "./upload.cjs"
  },
  "scripts": {
    "build": "node build.mjs",
    "start": "npm run build && node upload.cjs",
    "serve": "concurrently --kill-others './build.mjs watch' 'nodemon --quiet node upload.cjs serve --port 13481 --public-key-file public.pem'"
  },
  "author": "Lea Waller <lea@lea.science>",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.440.0",
    "@aws-sdk/s3-request-presigner": "^3.440.0",
    "@socket.io/admin-ui": "^0.5.1",
    "bytes": "^3.1.2",
    "commander": "^11.1.0",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "fastq": "^1.15.0",
    "format-duration": "^3.0.2",
    "gauge": "^5.0.1",
    "got": "^13.0.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "memoizee": "^0.4.15",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2"
  },
  "devDependencies": {
    "@types/bytes": "^3.1.3",
    "@types/debug": "^4.1.10",
    "@types/dotenv": "^8.2.0",
    "@types/format-duration": "^1.0.2",
    "@types/gauge": "^5.0.3",
    "@types/got": "^9.6.12",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/memoizee": "^0.4.10",
    "@types/node": "^20.8.10",
    "@types/socket.io": "^3.0.2",
    "@types/socket.io-client": "^3.0.0",
    "concurrently": "^8.2.2",
    "esbuild": "0.19.5",
    "nodemon": "^3.0.1",
    "typescript": "^5.2.2"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8",
    "utf-8-validate": "^5.0.10"
  }
}
